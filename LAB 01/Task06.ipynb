    "\n",
    "str = input(\"Enter a string: \")\n",
    "list = str.split()\n",
    "length = 0\n",
    "\n",
    "for word in list:\n",
    "    if len(word) > length:\n",
    "        length = len(word)\n",
    "        longestWord = word\n",
    "\n",
    "print(f\"Longest word: {longestWord} with length {length}\")\n"
